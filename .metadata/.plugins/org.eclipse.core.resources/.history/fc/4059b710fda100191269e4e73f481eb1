import java.util.ArrayList;

public class Box2<T> {
	private T name;

	public T getName() {
		return name;
	}

	public void setName(T name) {
		this.name = name;
	}
	
	public static void main(String[] args) {
		Box2<String> b = new Box2<>();
		b.setName("¾È³ç");
		System.out.println(b.getName());
		
		ArrayList<double> ab = new ArrayList<>();
	}	
}
