
public class BulgogiBurger {
	
	private int price;
	private String name;
	
	public BulgogiBurger() {
		this(4000, "불고기버거");
	}
	
	public BulgogiBurger(int price, String name) {
		super();
		this.price = price;
		this.name = name;
	}
	
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
