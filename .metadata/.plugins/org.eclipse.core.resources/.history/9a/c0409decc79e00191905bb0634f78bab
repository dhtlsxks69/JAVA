package protoss;

import protoss.DarkTempler;
import protoss.Dragoon;
import protoss.Gateway;
import protoss.Porge;
import protoss.Protoss;
import protoss.ProtossApp;
import protoss.Zealot;

public class ProtossApp {
	
	//누가, 누구를
	public static void attack(Protoss u1, Protoss u2){
		//질럿 공격력 : 16
		//드라군 방어력 : 1
		int damage = u1.getAttack()-u2.getArmor(); //15
		u2.setSh(u2.getSh() - damage);
	}
	
	public static void printUnit(Protoss u){
		System.out.println(u.getName() + "의 쉴드는 " + u.getSh());
	}
	
	//전장!!
	public static void main(String[] args) {
		//1. 배럭생성
		
		//2. 마린 2마리 생성 m1, m2
		
		//3. 파이어뱃 2마리 생성 f1, f2 (공격력 8)
		
		//4. m1, f2 전투
		
		//5. f1, f2 전투
		
		//6. f2의 체력환인
		
		//7. 공격력 업그레이드
		
		//8. 방어력 업그레이드
		
		//9. 마린의 공격력 방어력 확인
	}
}
