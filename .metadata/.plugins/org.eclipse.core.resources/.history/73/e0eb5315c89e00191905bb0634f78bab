package terran;

public class FireBat implements Terran{
	public FireBat() {
		this(100);
	}
	
	//매개변수의 타입이 다르거나, 개수가 다르면 오버로딩
	public FireBat(int st) {
		this.st = st;
		
		System.out.println("한판 붙어볼까요!");
	}
	
	final String name = "파이어뱃";
	int st; //체력
	static int attack = 8;
	static int armor = 1;
	
}
