package terran;

public class Marine implements Terran{
	public Marine() {
		this(100, 100);
	}
	
	//매개변수의 타입이 다르거나, 개수가 다르면 오버로딩
	public Marine(int st, int sh) {
		this.st = st;
		this.sh = sh;
		
		System.out.println("My Life Aior");
	}
	
	final String name = "마린";
	int st; //체력
	int sh; //쉴드(보호막)
	static int attack = 16;
	static int armor = 1;
}
